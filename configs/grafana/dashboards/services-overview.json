{
  "__inputs": [],
  "__elements": {},
  "__requires": [],
  "annotations": { "list": [] },
  "description": "Observability Demo - Service Overview & Business Metrics",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "title": "HTTP Service Metrics (RED)",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2 },
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "id": 2,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum(rate(http_server_duration_milliseconds_count{job=~\"api-gateway|user-service|order-service|notification-service\"}[1m])) by (job)",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ],
      "title": "Request Rate by Service (req/s)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2 },
          "unit": "percentunit",
          "max": 1,
          "min": 0
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "id": 3,
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum(rate(http_server_duration_milliseconds_count{http_status_code=~\"5..\",job=~\"api-gateway|user-service|order-service|notification-service\"}[1m])) by (job) / sum(rate(http_server_duration_milliseconds_count{job=~\"api-gateway|user-service|order-service|notification-service\"}[1m])) by (job)",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ],
      "title": "Error Rate by Service",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2 },
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 9 },
      "id": 4,
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.50, sum(rate(http_server_duration_milliseconds_bucket{job=~\"api-gateway|user-service|order-service|notification-service\"}[5m])) by (job, le))",
          "legendFormat": "p50 - {{job}}",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.95, sum(rate(http_server_duration_milliseconds_bucket{job=~\"api-gateway|user-service|order-service|notification-service\"}[5m])) by (job, le))",
          "legendFormat": "p95 - {{job}}",
          "refId": "B"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.99, sum(rate(http_server_duration_milliseconds_bucket{job=~\"api-gateway|user-service|order-service|notification-service\"}[5m])) by (job, le))",
          "legendFormat": "p99 - {{job}}",
          "refId": "C"
        }
      ],
      "title": "Latency Percentiles (p50 / p95 / p99)",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 17 },
      "id": 5,
      "title": "Business Metrics",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "thresholds" }, "unit": "short" },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 18 },
      "id": 6,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "orders_created_total",
          "legendFormat": "Orders Created",
          "refId": "A"
        }
      ],
      "title": "Total Orders Created",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "thresholds" }, "unit": "short" },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 18 },
      "id": 7,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "users_created_total",
          "legendFormat": "Users Created",
          "refId": "A"
        }
      ],
      "title": "Total Users Created",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "thresholds" }, "unit": "short" },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 6, "x": 12, "y": 18 },
      "id": 8,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "notifications_sent_total",
          "legendFormat": "Notifications Sent",
          "refId": "A"
        }
      ],
      "title": "Total Notifications Sent",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "thresholds" }, "unit": "short" },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 6, "x": 18, "y": 18 },
      "id": 9,
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "orders_errors_total",
          "legendFormat": "Order Errors",
          "refId": "A"
        }
      ],
      "title": "Total Order Errors",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2 },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 23 },
      "id": 10,
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "rate(orders_created_total[5m]) * 60",
          "legendFormat": "Orders/min",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "rate(notifications_sent_total[5m]) * 60",
          "legendFormat": "Notifications/min",
          "refId": "B"
        }
      ],
      "title": "Orders & Notifications Rate",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2 },
          "unit": "currencyUSD"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 23 },
      "id": 11,
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.50, sum(rate(order_value_bucket[5m])) by (le))",
          "legendFormat": "p50 Order Value",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.95, sum(rate(order_value_bucket[5m])) by (le))",
          "legendFormat": "p95 Order Value",
          "refId": "B"
        }
      ],
      "title": "Order Value Distribution",
      "type": "timeseries"
    }
  ],
  "refresh": "10s",
  "schemaVersion": 38,
  "tags": ["observability", "microservices", "opentelemetry"],
